            init() {
                this.setupEventListeners();
                this.createParticles();
                this.updateTime();
                this.showWelcomeMessage();
            }
        
            setupEventListeners() {
                document.querySelectorAll('.hack-btn').forEach(btn => {
                    btn.addEventListener('click', (e) => this.executeHack(e));
                });
                setInterval(() => this.updateTime(), 1000);
            }
        
            async executeHack(event) {
                if (this.isExecuting) return;
        
                const button = event.currentTarget;
                const hackName = button.dataset.hack;
                const commands = this.hackCommands[hackName];
        
                if (!commands) return;
        
                this.isExecuting = true;
                button.classList.add('clicked');
                this.progressContainer.style.display = 'block';
                this.progressFill.style.width = '0%';
        
                for (let i = 0; i < commands.length; i++) {
                    await this.logLine(commands[i]);
                    let progress = ((i + 1) / commands.length) * 100;
                    this.progressFill.style.width = progress + '%';
                    this.progressText.textContent = `Executing ${hackName}... ${Math.round(progress)}%`;
                    await this.delay(600);
                }
        
                this.progressText.textContent = `${hackName.toUpperCase()} completed!`;
                await this.delay(800);
        
                this.progressContainer.style.display = 'none';
                button.classList.remove('clicked');
                this.isExecuting = false;
            }
        
            logLine(text) {
                return new Promise(resolve => {
                    const line = document.createElement('div');
                    line.className = 'terminal-line';
                    line.textContent = text;
                    this.terminal.appendChild(line);
                    this.terminal.scrollTop = this.terminal.scrollHeight;
                    setTimeout(resolve, 200);
                });
            }
        
            delay(ms) { return new Promise(resolve => setTimeout(resolve, ms)); }
        
            updateTime() {
                const now = new Date();
                const timeString = now.toLocaleTimeString('vi-VN', { hour12: false });
                document.getElementById('currentTime').textContent = timeString;
            }
        
            showWelcomeMessage() {
                this.logLine('=== WELCOME TO AIMLOCK MENU FAROS ===');
                this.logLine('Initializing system...');
            }
        
            createParticles() {
                const particlesContainer = document.getElementById('particles');
                for (let i = 0; i < 80; i++) {
                    const particle = document.createElement('div');
                    particle.className = 'particle';
                    particle.style.left = Math.random() * 100 + 'vw';
                    particle.style.animationDuration = (5 + Math.random() * 10) + 's';
                    particle.style.opacity = Math.random();
                    particle.style.width = particle.style.height = (1 + Math.random() * 3) + 'px';
                    particlesContainer.appendChild(particle);
                }
            }
        }
        
        document.addEventListener('DOMContentLoaded', () => {
            new GameModInterface();
        });
        </script>
        
